{
  "name": "@capybara-plus/icons-vue",
  "version": "1.0.4",
  "description": "A Icon Vue Component for CapybaraUI",
  "main": "./dist/index.js",
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build": "pnpm run build:generate && run-p build:build build:types",
    "build:generate": "tsx ./build/generate.ts",
    "build:build": "tsx build/build.ts",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly"
  },
  "keywords": [
    "capybaraui",
    "icons",
    "vue"
  ],
  "author": "Ranaran",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "./*",
        "./dist/types/*"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/capybaraui/icons.git"
  },
  "bugs": {
    "url": "https://github.com/capybaraui/icons/issues"
  },
  "homepage": "https://github.com/capybaraui/icons#readme",
  "peerDependencies": {
    "vue": "^3.4.21"
  },
  "dependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "vue-tsc": "^2.0.6"
  }
}
